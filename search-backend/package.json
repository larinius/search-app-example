{
  "name": "express-typescript-boilerplate",
  "version": "1.0.14",
  "description": "An Express boilerplate backend",
  "author": "Edwin Hernandez",
  "repository": "edwinhern/express-typescript-2024",
  "license": "MIT",
  "main": "index.ts",
  "private": true,
  "scripts": {
    "dev": "tsx watch --clear-screen=false src/index.ts | pino-pretty",
    "build": "tsup",
    "start": "node dist/index.js",
    "clean": "rimraf dist coverage",
    "lint": "biome check src/",
    "lint:fix": "biome check src/ --fix",
    "format": "biome format src/",
    "seed": "ts-node src/utils/seedProducts.js"
  },
  "dependencies": {
    "@apollo/server": "^4.12.2",
    "@graphql-tools/merge": "^9.0.24",
    "@graphql-tools/schema": "^10.0.23",
    "@types/graphql-upload": "^17.0.0",
    "aws-sdk": "^2.1692.0",
    "axios": "^1.7.9",
    "bcryptjs": "^3.0.2",
    "chance": "^1.1.13",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "envalid": "^8.0.0",
    "express": "^4.19.2",
    "express-rate-limit": "^7.2.0",
    "graphql": "^16.11.0",
    "graphql-depth-limit": "^1.1.0",
    "graphql-playground-middleware-express": "^1.7.23",
    "graphql-redis-subscriptions": "^2.7.0",
    "graphql-upload": "^17.0.0",
    "graphql-upload-ts": "^2.1.2",
    "graphql-validation-complexity": "^0.4.2",
    "graphql-ws": "^6.0.5",
    "helmet": "^8.0.0",
    "http-status-codes": "^2.3.0",
    "ioredis": "^5.6.1",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.17.0",
    "node-cron": "^3.0.3",
    "pg": "^8.13.1",
    "pino": "^9.4.0",
    "pino-http": "^10.0.0",
    "reflect-metadata": "^0.2.2",
    "sharp": "^0.34.2",
    "sqlite3": "^5.1.7",
    "swagger-ui-express": "^5.0.0",
    "tsconfig-paths": "^4.2.0",
    "typeorm": "^0.3.24",
    "ws": "^8.18.2"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@faker-js/faker": "^9.8.0",
    "@types/aws-sdk": "^0.0.42",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/graphql-depth-limit": "^1.1.6",
    "@types/graphql-validation-complexity": "^0.4.4",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^24.0.2",
    "@types/node-cron": "^3.0.11",
    "@types/sharp": "^0.31.1",
    "@types/supertest": "^6.0.2",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/ws": "^8.18.1",
    "@vitest/coverage-v8": "^2.1.8",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "pino-pretty": "^13.0.0",
    "rimraf": "^6.0.0",
    "supertest": "^7.0.0",
    "ts-node": "^10.9.2",
    "tsup": "^8.0.2",
    "tsx": "^4.7.2",
    "typescript": "^5.8.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^2.0.0"
  },
  "lint-staged": {
    "*.{js,ts,cjs,mjs,d.cts,d.mts,json,jsonc}": [
      "biome check --apply --no-errors-on-unmatched"
    ]
  },
  "tsup": {
    "entry": [
      "src",
      "!src/**/__tests__/**",
      "!src/**/*.test.*"
    ],
    "splitting": false,
    "sourcemap": true,
    "clean": true
  }
}
